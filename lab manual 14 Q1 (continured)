#include <stdio.h>
#include <string.h>
void str_cat(char str1[], char str2[],int length1,int length2, int n);
int str_length(char arr[]);
int main()
{
    int n,arr1,length1,length2;
    char str1[100];
    char str2[100];
    gets(str1);
    gets(str2);
    length1=str_length(str1);
    length2=str_length(str2);
    printf("enter the number\n");
    scanf("%d",&n);
    str_cat(str1,str2,length1,length2,n);
    return 0;
}
int str_length(char *arr){
    int i;
    for(i=0;arr[i]!='\0';i++){
        return i;
    }
    
}
void str_cat(char str1[], char str2[],int length1,int length2, int n){
    strcat(str1,str2);
    
}
