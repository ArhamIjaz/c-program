#include <stdio.h>
int power(int x, int y);
int main(){
int x,y,result;
printf("enter x: ");
scanf("%d",&x);
printf("enter y: ");
scanf("%d",&y);
result=power(x,y);
printf("power is %d",result);

return 0;}

int power(int x, int y){
    int p=1;
    for(int i=0;i<y;i++){
        p=p*x;
    }
return p;
}
